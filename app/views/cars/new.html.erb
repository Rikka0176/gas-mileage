<h1>新しい車種情報を追加</h1>
<%= form_with(model: @car, url: cars_path) do |form| %>
  <div>
    <%= form.label :make, "メーカー" %>
    <%= form.text_field :make %>
  </div>
  <div>
    <%= form.label :model, "モデル" %>
    <%= form.text_field :model %>
  </div>
  <div>
    <%= form.label :year, "年式" %>
    <%= form.number_field :year %>
  </div>
  <div>
    <%= form.label :mileage, "燃費" %>
    <%= form.number_field :mileage %>
  </div>
  <div>
    <%= form.submit "追加" %>
  </div>
<% end %>